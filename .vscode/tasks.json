{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Quasar Dev",
      "type": "npm",
      "script": "dev",
      "isBackground": true,
      "problemMatcher": {
        "owner": "quasar",
        "pattern": {
          "regexp": "."
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": "^.*Starting dev server.*$",
          "endsPattern": "^.*App URL.*http://localhost.*$"
        }
      },
      "presentation": {
        "reveal": "always",
        "panel": "new"
      }
    }
  ]
}
